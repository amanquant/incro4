{
  "version": 2,
  "env": {
    "SUPABASE_URL": "",
    "SUPABASE_API_KEY": "",
    "SUPABASE_SERVICE_KEY": "",
    "VERCEL_ENV": ""
  },
  "buildCommand": "pip install -r requirements.txt",
  "outputDirectory": ".",
  "functions": {
    "api/app.py": {
      "runtime": "vercel/python3.11"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/app.py"
    }
  ]
}
